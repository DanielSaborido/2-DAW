#!/usr/bin/python3
print("Content-type: text/html")
print("")
print('<html>')
print('<head>')
print('<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">')
print('<title>Hello World</title>')
print('</head>')
print('<body>')
print('Python Rules!!!!')
print('</body>')
print('</html>')
exit(0)
